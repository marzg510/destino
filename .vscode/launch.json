{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "life_game_app(web)",
      "cwd": "life_game_app",
      "request": "launch",
      "type": "dart",
      "deviceId": "web-server",
      "args": [
        "--web-port",
        "3000",
      ]
    },
    {
      "name": "life_game_app(web-debug)",
      "cwd": "life_game_app",
      "request": "launch",
      "type": "dart",
      "deviceId": "web-server",
      "flutterMode": "debug",
      "args": [
        "--web-port",
        "3000",
        "--dart-define=FLUTTER_WEB_USE_SKIA=true"
      ]
    },
    {
      "name": "life_game_app (profile mode)",
      "cwd": "life_game_app",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "life_game_app (release mode)",
      "cwd": "life_game_app",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}